            qty=poe14a.qty_received
            iv_extension=0
            if pom02a.line_type$="O" qty=1
            precision 2
            po_extension=poe14a.unit_cost*qty
            if pom02a.line_type$="S" iv_extension=ivm02a.unit_cost*qty*poe14a.conv_factor
            precision i[1]

rem --- Branch based on line code

            if pos(pom02a.line_type$="SNVMO") = 0 break
            on pos(pom02a.line_type$="SNVMO")-1 goto std_line,non_stock,vend_part,msg_line,other

std_line: rem --- Standard line

            cust$=""
    
            OutVect!.addItem("")
            OutVect!.addItem(poe14a.po_line_code$)
            OutVect!.addItem(poe14a.warehouse_id$)
            OutVect!.addItem(poe14a.item_id$)
            OutVect!.addItem(code$)
            OutVect!.addItem(poe14a.unit_measure$)
            OutVect!.addItem(str(poe14a.qty_ordered:m2$)+$00$+"R")
            OutVect!.addItem(str(poe14a.qty_received:m2$)+$00$+"R")
            OutVect!.addItem(str(ivm02a.unit_cost*poe14a.conv_factor:m3$)+$00$+"R")
            OutVect!.addItem(str(iv_extension:m1$)+$00$+"R")
            OutVect!.addItem(str(poe14a.unit_cost:m3$)+$00$+"R")
            OutVect!.addItem(str(po_extension:m1$)+$00$+"R")
            OutVect!.addItem(poe14a.location$)

            recs=recs+1
            xvk=fnblank(3)
            OutVect!.addItem(description$)
            xvk=fnblank(9)

            l=l+2
            goto lot_ser


            OutVect!.addItem("")
            OutVect!.addItem(poe14a.po_line_code$)
            OutVect!.addItem(poe14a.warehouse_id$)
            xvk=fnblank(7)
            OutVect!.addItem(str(poe14a.unit_cost))
            OutVect!.addItem(str(po_extension))

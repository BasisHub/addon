	<sql_select>SELECT pot_rechdr.firm_id, pot_rechdr.po_no, pot_rechdr.receiver_no, pot_rechdr.vendor_id, apm_vendmast.vendor_name, pot_rechdr.warehouse_id, ivc_whsecode.short_name, pot_rechdr.po_frt_terms, pot_rechdr.ap_ship_via, pot_rechdr.acknowledge, pot_rechdr.fob, pot_rechdr.req_no, poe_reqhdr.vendor_id, pot_rechdr.ord_date, pot_rechdr.recpt_date[$0a$]FROM pot_rechdr[$0a$]LEFT JOIN apm_vendmast ON apm_vendmast.firm_id=pot_rechdr.firm_id AND apm_vendmast.vendor_id=pot_rechdr.vendor_id[$0a$]LEFT JOIN ivc_whsecode ON ivc_whsecode.firm_id=pot_rechdr.firm_id AND ivc_whsecode.record_id_c='C' AND ivc_whsecode.warehouse_id=pot_rechdr.warehouse_id[$0a$]LEFT JOIN poe_reqhdr ON poe_reqhdr.firm_id=pot_rechdr.firm_id AND poe_reqhdr.req_no=pot_rechdr.req_no[$0a$]                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     
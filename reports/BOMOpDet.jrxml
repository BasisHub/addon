from bmm_billoper
left join bmc_opcodes on bmm_billoper.firm_id = bmc_opcodes.firm_id and bmm_billoper.op_code = bmc_opcodes.op_code
where firm_id = $P{FIRM_ID} and bill_no = $P{BILL_NO}]]>

							call pgmdir$+"adc_getmask.aon::enter_with_flag","","AR","A","",amt_mask$,0,0,"N"
							call pgmdir$+"adc_getmask.aon::enter_with_flag","","AR","U","",unit_mask$,0,0,"N"
